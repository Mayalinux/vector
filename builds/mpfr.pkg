[files]
url=http://www.mpfr.org/mpfr-current/
file=mpfr-3.1.2.tar.xz

[build]

# Ensure GMP is installed in env

./configure --prefix=/usr --sysconfdir=/etc --localstatedir=/var --disable-maintainer-mode --with-sysroot=/home/qafir/linux/env/ CFLAGS="-I/home/qafir/linux/env/usr/include" --with-gmp-lib=/home/qafir/linux/env/usr/lib/

make -j5

[install]
make DESTDIR=/tmp/pkg/ install

