[files]
url=ftp://ftp.kernel.org/pub/linux/utils/kbd/
url=kbd-project.org
file=kbd-2.0.1.tar.xz

[build]
./configure --prefix=/usr --sysconfdir=/etc --localstatedir=/var --disable-rpath --enable-optional-progs --enable-libkeymap --with-sysroot=/home/qafir/linux/env CFLAGS="-I/home/qafir/linux/env/usr/include"

make -j5

[install]
make DESTDIR=/tmp/pkg install
