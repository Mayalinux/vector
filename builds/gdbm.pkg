[files]
url=http://www.gnu.org.ua/software/gdbm/
file=gdbm-1.11.tar.gz

[build]
./configure --prefix=/usr --sysconfdir=/etc --localstatedir=/var CFLAGS="-I/home/qafir/linux/env/usr/include" --enable-shared --enable-static --disable-rpath --with-sysroot=/home/qafir/linux/env 

make

[install]
make DESTDIR=/tmp/pkg install
